{
    "name": "@locr-company/vms2-tile-db-reader",
    "version": "1.0.0",
    "description": "A class for reading tile data from a database.",
    "type": "module",
    "repository": {
        "type": "git",
        "url": "https://github.com/locr-company/js-vms2-tile-db-reader"
    },
