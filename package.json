    "keywords": [
        "vms2"
    ],
    "author": "Ringo Leese <r.leese@locr.com>",
    "license": "MIT",
    "engines": {
        "node": ">=18"
    },
    "dependencies": {
        "better-sqlite3": "^11.8.1"
    },
    "devDependencies": {
        "@types/better-sqlite3": "^7.6.12",
        "@types/mocha": "^10.0.6",
        "@types/node": "^22.13.4",
        "c8": "^10.1.2",
        "chai": "^5.1.0",
        "mocha": "^11.0.1",
        "node-gyp": "^11.0.0",
        "typescript": "^5.4.3"
    },
    "c8": {
        "all": true,
        "include": [
            "src/**/*.js"
